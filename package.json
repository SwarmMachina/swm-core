{
  "name": "@swarmmachina/swm-core",
  "version": "1.0.0",
  "description": "Zero-dependency high-performance HTTP/WebSocket server built on uWebSockets.js",
  "keywords": [
    "http",
    "websocket",
    "server",
    "zero-dependency",
    "performance",
    "swarmmachina",
    "uwebsockets",
    "fast"
  ],
  "author": "SwarmMachina Team",
  "license": "MPL-2.0",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SwarmMachina/swm-core.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "check": "prettier . --check && eslint . --ext .js,.vue",
    "fix": "prettier . --write && eslint . --ext .js,.vue --fix",
    "test": "node --import ./tests/helpers/register-loader.js --test tests/**/*.test.js",
    "test:coverage": "node --import ./tests/helpers/register-loader.js --test --experimental-test-coverage tests/**/*.test.js",
    "bench:base": "node ./benchmark/run-bench.js base all",
    "bench:post": "node ./benchmark/run-bench.js post-base all",
    "bench:base:profile": "node ./benchmark/run-bench.js base all --profile",
    "bench:post:profile": "node ./benchmark/run-bench.js post-base all --profile",
    "bench:list": "node ./benchmark/run-bench.js --list-tests",
    "prepublishOnly": "npm run fix && npm test",
    "release": "npm run fix && npm test && npm publish"
  },
  "dependencies": {
    "uwebsockets.js": "uNetworking/uWebSockets.js#v20.56.0"
  },
  "devDependencies": {
    "@swarmmachina/standards": "github:SwarmMachina/standards#semver:^1.0.0",
    "autocannon": "^8.0.0",
    "express": "^5.2.1",
    "fastify": "^5.6.2",
    "micro": "^10.0.1",
    "ws": "^8.18.3"
  }
}
