{
  "name": "@swarmmachina/swm-core",
  "version": "1.1.4",
  "description": "Zero-dependency high-performance HTTP/WebSocket server built on uWebSockets.js",
  "keywords": [
    "http",
    "websocket",
    "server",
    "zero-dependency",
    "performance",
    "swarmmachina",
    "uwebsockets",
    "fast"
  ],
  "author": "SwarmMachina Team",
  "license": "MPL-2.0",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SwarmMachina/swm-core.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check": "prettier . --check && eslint . --ext .js,.vue",
    "fix": "prettier . --write && eslint . --ext .js,.vue --fix",
    "test": "node --import ./tests/helpers/register-loader.js --test tests/unit/*.test.js",
    "test:coverage": "node --import ./tests/helpers/register-loader.js --test --experimental-test-coverage tests/unit/*.test.js",
    "test:e2e": "node --test tests/e2e/**/*.test.js",
    "test:e2e:http": "node --test tests/e2e/http/*.test.js",
    "test:e2e:ws": "node --test tests/e2e/ws/*.test.js",
    "bench": "node ./benchmark/bench.js --test base --runs 1 --warmup 10 --sample-ms 250 --v8prof true",
    "bench:core": "node ./benchmark/bench.js --test base --runs 1 --warmup 10 --sample-ms 250 --v8prof true --fw core",
    "prepublishOnly": "npm run fix && npm test",
    "release": "npm run check && npm test && npm publish"
  },
  "dependencies": {
    "uwebsockets.js": "uNetworking/uWebSockets.js#v20.56.0"
  },
  "devDependencies": {
    "@swarmmachina/standards": "^1.0.1",
    "autocannon": "^8.0.0",
    "express": "^5.2.1",
    "fastify": "^5.6.2",
    "micro": "^10.0.1",
    "ws": "^8.18.3"
  }
}
